<html dir="rtl" lang="fa-IR">
<head>
    <meta charset="utf-8" />
    @RenderSection("ContentOptimization", false)
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />

    <!-- CSS Global Compulsory -->
    <link rel="stylesheet" href="/Content/unify/assets/plugins/bootstrap/css/bootstrap-rtl.min.css">
    <link rel="stylesheet" href="/Content/unify/assets/css/css-rtl/shop.style-rtl.css">
    <!-- CSS Header and Footer -->
    <link rel="stylesheet" href="/Content/unify/assets/css/css-rtl/headers/header-v5-rtl.css">
    <link rel="stylesheet" href="/Content/unify/assets/css/css-rtl/footers/footer-v4-rtl.css">
    <!-- CSS Implementing Plugins-->
    <link rel="stylesheet" href="/Content/unify/assets/plugins/animate.css">
    <link rel="stylesheet" href="/Content/unify/assets/plugins/line-icons/line-icons.css">
    <link rel="stylesheet" href="/Content/unify/assets/plugins/font-awesome/css/font-awesome.min.css">
    <link rel="stylesheet" href="/Content/unify/assets/plugins/scrollbar/css/jquery.mCustomScrollbar.css">
    <link href="~/Content/beyond/assets/css/toaster/v2.1.3/toastr.css" rel="stylesheet" />
    @Styles.Render("/Scripts/jqueryConfirm/jquery-confirm.min.css")

    @Styles.Render("/unify/CSS/SkyForms")

    @RenderSection("CssPlugins", false)
    <!-- Style Switcher -->
    <link rel="stylesheet" href="/Content/unify/assets/css/plugins/style-switcher.css">
    <!-- CSS Theme -->
    <link rel="stylesheet" href="/Content/unify/assets/css/theme-colors/dark-red.css" id="style_color">
    <!-- RTL Customization -->
    <link rel="stylesheet" href="/Content/unify/assets/css/css-rtl/rtl.css">
    <!-- CSS Customization -->
    <link rel="stylesheet" href="/Content/unify/assets/css/css-rtl/custom-rtl.css">
    <link href="~/Content/css/custom.css" rel="stylesheet" />
    <link href="~/Content/fonts.css" rel="stylesheet" />
    <link href="~/Content/css/custom-user-page.css" rel="stylesheet" />
    @RenderSection("Styles", false)
</head>
<body class="header-fixed">
    <div class="wrapper">
        @Html.Partial("~/Views/Shared/Layout/_Header.cshtml")
        <noscript>
            <div class="margin-top-10 container">
                <div class="row">
                    <div class="alert alert-danger fade in">
                        <strong>خطا ! </strong> جاوا اسکریپت مرورگر شما غیرفعال است یا جاوا اسکریپت را اجرا نمی کند، لطفاً جاوا اسکریپت را برروی مرورگر خود را فعال نمایید.
                    </div>
                </div>
            </div>
        </noscript>
        @RenderSection("ContentTop", required: false)
        <div class="margin-top-9 container content profile">
            <div class="row">
                @Html.Partial("~/Areas/User/Views/Shared/MainUser/Menu.cshtml")
                <div class="col-md-9">
                    @RenderBody()
                </div>
            </div>
        </div>
        @Html.Partial("~/Views/Shared/Layout/_Footer.cshtml")
    </div>

    <!-- JS Global Compulsory -->
    <script src="~/Content/unify/assets/plugins/jquery/jquery.min.js"></script>
    <script src="~/Content/unify/assets/plugins/jquery/jquery-migrate.min.js"></script>
    <script src="~/Content/unify/assets/plugins/bootstrap/js/bootstrap.min.js"></script>
    <!-- JS Implementing Plugins -->
    <script src="~/Content/unify/assets/plugins/back-to-top.js"></script>
    <script src="~/Content/unify/assets/plugins/smoothScroll.js"></script>
    <script src="~/Content/unify/assets/plugins/jquery.parallax.js"></script>
    <script src="~/Content/unify/assets/plugins/owl-carousel2/owl.carousel.min.js"></script>
    <script src="~/Content/unify/assets/plugins/scrollbar/js/jquery.mCustomScrollbar.concat.min.js"></script>
    <script src="~/Content/beyond/assets/js/toastr/v2.1.3/toastr.js"></script>
    <script src="~/Scripts/AlertLoading/PAlert.js"></script>
    <script src="~/Scripts/spin.min.js"></script>
    <script src="~/Scripts/AlertLoading/PLoading.js"></script>
    <script src="~/Scripts/jqueryConfirm/jquery-confirm.min.js"></script>

    @RenderSection("JSPlugins", false)

    @Scripts.Render("~/bundles/js/jqueryval")

    <!-- JS Customization -->
    <script src="~/Content/unify/assets/js/custom.js"></script>
    <!-- JS Page Level -->
    <script src="~/Content/unify/assets/js/shop.app.js"></script>
    <script src="~/Scripts/serializeJSON.js"></script>
    @RenderSection("JSPageLevel", false)

    <script>
        jQuery(document).ready(function () {
            App.init();
            App.initScrollBar();
        });
    </script>
    <!--[if lt IE 9]>
        <script src="../assets/plugins/respond.js"></script>
        <script src="../assets/plugins/html5shiv.js"></script>
        <script src="../assets/js/plugins/placeholder-IE-fixes.js"></script>
        <![endif]-->


    @RenderSection("Scripts", false)
    @RenderSection("FooterPage", false)
    <script src="~/Scripts/Site/ShoppingCart.js"></script>
</body>
</html>